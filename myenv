#!/bin/bash

# 作为工具的总入口
# 设定当前的环境
#  myenv <command> [options]
#    如果没有 参数，那么找 nearest env。
# 工作
#  mysh <command> [options]
#     如果没有 myenv，那么就选择 nearest env。

export MYENV_TOOL_PATH="$(cd "$(dirname "$0")" && pwd)"
export MYENV_TOOL_SRC_PATH="${MYENV_TOOL_PATH}/src"

source ${MYENV_TOOL_SRC_PATH}/common/pre_parse.sh

source ${MYENV_TOOL_VERSION_DIR}/parse.sh

source ${MYENV_TOOL_SRC_PATH}/common/post_parse.sh

source ${MYENV_TOOL_VERSION_DIR}/run.sh
